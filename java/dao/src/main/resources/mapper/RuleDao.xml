<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com._365d1.dao.RuleDao">

    <select id="selectMenu" resultType="com._365d1.model.Rule">
        SELECT
            *
        FROM
            rule
        WHERE
            page = 0
        ORDER BY
            sort ASC
    </select>

</mapper>
